   </BOUNDS>
   <PIN pinname="Input String" slicecount="1" visible="0" values="|CODE&cr;&lf;do forever (&cr;&lf; if (currentstate=reset) and (input=OnSet) then (currentstate:=set) and (DoSet)&cr;&lf; if (currentstate=set) and (input=OnReset) then (currentstate:=reset) and (DoReset)&cr;&lf;)|">
   </PIN>
